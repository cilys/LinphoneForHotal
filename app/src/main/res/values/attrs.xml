<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="remoteview">
        <attr name="view_width" format="integer" />
        <attr name="view_height" format="integer" />
        <attr name="backgroundColor" format="reference|color" />
        <attr name="foregroundColor" format="reference|color" />
        <attr name="topArrowColor" format="reference|color" />
        <attr name="rightArrowColor" format="reference|color" />
        <attr name="bottomArrowColor" format="reference|color" />
        <attr name="leftArrowColor" format="reference|color" />
        <attr name="centerTextColor" format="reference|color" />
        <attr name="centerTextSize" format="dimension" />
        <attr name="rectColor" format="reference|color" />
        <attr name="touchForegroundColor" format="reference|color" />
        <attr name="touchArrowColor" format="reference|color" />
    </declare-styleable>
    
    <declare-styleable name="dashview">
        <attr name="arcWidth" format="dimension" />
        <attr name="arcDarkColor" format="reference|color" />
        <attr name="arcLightColor" format="reference|color" />
        <attr name="arcPointCenterColor" format="reference|color" />
        <attr name="arcPointRingColor" format="reference|color" />
        <attr name="centerCircleColor" format="reference|color" />
        <attr name="dashviewBackgroundColor" format="reference|color" />
        <attr name="dashviewCenterTextSize" format="dimension" />
    </declare-styleable>
</resources>